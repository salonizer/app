{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@ngx-pdb/pdb-wrapper": [
        "dist/ngx-pdb/pdb-wrapper"
      ],
      "@ngx-pdb/pdb-wrapper/*": [
        "dist/ngx-pdb/pdb-wrapper/*"
      ],
      "@ngx-dnd/pdb": [
        "dist/ngx-dnd/pdb"
      ],
      "@ngx-dnd/pdb/*": [
        "dist/ngx-dnd/pdb/*"
      ]
    }
  }
}